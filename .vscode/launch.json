{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Editor-Win32-Debug",
      "type": "cppvsdbg",
      "request": "launch",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/Build/Debug",
      "program": "${workspaceFolder}/Build/Debug/Editor.exe"
    },
    {
      "name": "Runtime-Win32-Debug",
      "type": "cppvsdbg",
      "request": "launch",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/Build/Debug",
      "program": "${workspaceFolder}/Build/Debug/Runtime.exe"
    },
    {
      "name": "Editor-Linux-Debug",
      "type": "cppdbg",
      "request": "launch",
      "miDebuggerPath": "/usr/bin/gdb",
      "cwd": "${workspaceFolder}/Build/Debug",
      "program": "${workspaceFolder}/Build/Debug/Editor"
    },
    {
      "name": "Runtime-Linux-Debug",
      "type": "cppdbg",
      "request": "launch",
      "miDebuggerPath": "/usr/bin/gdb",
      "cwd": "${workspaceFolder}/Build/Debug",
      "program": "${workspaceFolder}/Build/Debug/Runtime"
    }
  ]
}